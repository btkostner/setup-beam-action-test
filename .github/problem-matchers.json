{
  "problemMatcher": [
    {
      "owner": "elixir-mixCompileError",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\*\\* \\((\\w+)\\) (.*):(\\d+): (.*)$",
          "file": 2,
          "line": 3,
          "message": 0
        }
      ]
    },
    {
      "owner": "elixir-mixCompileWarning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^warning: (.*)$",
          "message": 1
        },
        {
          "regexp": "^  (.*):(\\d+).*$",
          "file": 1,
          "line": 2
        }
      ]
    },
    {
      "owner": "elixir-mixTestFailure",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\s*\\d+\\) (.*)$"
        },
        {
          "regexp": "^\\s*(.*):(\\d+)$",
          "file": 1,
          "line": 2
        },
        {
          "regexp": "^\\s+(.*)$",
          "message": 1,
          "loop": true
        }
      ]
    },
    {
      "owner": "elixir-credoCheckWarnings",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^┃\\s\[\\w\]\\s→\\s(.*)$",
          "message": 1
        },
        {
          "regexp": "^┃\\s{7}(.*):(\\d+):(\\d+)\\s.*$",
          "file": 1,
          "line": 2,
          "column": 3
        }
      ]
    }
  ]
}
